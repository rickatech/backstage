<?xml version="1.0" encoding="UTF-8"?>

<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="OnLoadRibonUI">
  <backstage>
    <!--onShow="Backstage_OnShow">-->
    <tab idMso="TabSave">
      <firstColumn>
        <taskFormGroup idMso="SaveGroup">
          <category idMso="Save">
            <task id="myCustomTask1" label="ProLoop" insertAfterMso="ButtonTaskDynamicServiceProvider"
                  getImage="GetButtonImage">
              <!--imageMso="Folder"-->
              <group id="myGroupInTabSave">
                <!--<topItems>-->
                <!--<layoutContainer id="layout1" layoutChildren="vertical">-->
                <primaryItem>
                  <button id="sabreBrowseFolder" label="WebDAV Folder Browse" imageMso="Folder"
                          onAction="SaveAsButton_Click" />
                </primaryItem>
                <!--</layoutContainer>-->
                <!--</topItems>-->
              </group>
            </task>
          </category>
        </taskFormGroup>
      </firstColumn>
    </tab>

    <tab idMso="TabRecent">
      <firstColumn>
        <taskFormGroup idMso="GroupOpenPlaces">
          <category idMso="ButtonCategoryRecentDocuments">
            <task id="myCustomTask2" label="ProLoop" insertAfterMso="ButtonTaskDynamicServiceProvider"
                  getImage="GetButtonImage">
              <!--image="proloop" >-->
              <group id="myGroupInTabRecent">
                <!--<topItems>
                  <layoutContainer id="layout2" layoutChildren="vertical">-->
                <primaryItem>
                  <button id="PropertySheet" label="WebDAV Folder Browse" imageMso="Folder" onAction="OpenButton_Click" />
                </primaryItem>
                <!--</layoutContainer>
                </topItems>-->
              </group>
            </task>
          </category>
        </taskFormGroup>
      </firstColumn>
    </tab>

  </backstage>
</customUI>